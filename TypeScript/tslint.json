{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-name": false,
      "member-access": [true, "no-public"],
      "prefer-const": false,
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "trailing-comma": false
    },
    "rulesDirectory": []
}
